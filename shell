## Yahoo
    echo "dataset = Yahoo ...." >> graphchi_metrics.txt.statistics 
    echo "app = PageRank, dataset = Yahoo from echo" >> graphchi_metrics.txt.statistics 
    echo "R = 1×N, L = 20, choose prob=0.2 from echo" >> graphchi_metrics.txt.statistics 
    for(( times = 0; times < 5; times++))
    do
        echo "times = " $times " from echo"
        ./bin/apps/pagerank file "../DataSet/Yahoo/yahoo-webmap.txt" nvertices 1413511390 nedges 6636600779
    done

    echo "app = PersonalizedPageRank, dataset = Yahoo from echo" >> graphchi_metrics.txt.statistics 
    echo "R = 1000, L = 10, choose prob=0.2 from echo" >> graphchi_metrics.txt.statistics 
    for(( times = 0; times < 5; times++))
    do
        echo "times = " $times " from echo"
        ./bin/apps/personalizedpagerank file "../DataSet/Yahoo/yahoo-webmap.txt" nvertices 1413511390 nedges 6636600779
    done